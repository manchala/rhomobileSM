<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @inventory.Sku %></h1>
    <%= link_to "Edit", :action => :edit, :id => @inventory.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">Sku</td>
        <td class="itemValue"><%= @inventory.Sku%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Quantity</td>
        <td class="itemValue"><%= @inventory.Quantity%></td>
      </tr>
      
    </table>
  </div>

</div>
